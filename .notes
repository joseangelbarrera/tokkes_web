// Export DB from mongo local
mongoexport -d tokkes -c products -o products.json

// Import db to mongo local
mongoimport -d tokkes -c products --file products.json

// Import DB to mLab
mongoimport -h ds115352.mlab.com:15352 -d tokkes-products -c products -u admin -p 100 --file products.json

// Set heroku DB_URI
heroku config:set DB_URI=mongodb://admin:100@ds115352.mlab.com:15352/tokkes-products

